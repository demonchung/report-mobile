  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "dev:m": "cross-env  npm_config_is_mobile=true npm run dev",
    "build": "npm run dist && npm run dist:lib",
    "build:m": "npm run mdist && npm run mdist:lib",
    "build:publish": "npm run dist && npm run dist:lib && npm publish",
    "build:m:publish": "npm run mdist && npm run mdist:lib && npm publish",
    "doc": "vuepress build develop/docs --no-cache",
    "doc:dev": "vuepress dev develop/docs --no-cache",
    "mock:pc": "cross-env NODE_ENV=mock webpack-dev-server --config build/webpack.dev.config.js",
    "mock:m": "cross-env NODE_ENV=mock npm_config_is_mobile=true webpack-dev-server --config build/webpack.dev.config.js",
    "dist": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.dist.config.js",
    "mdist": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.m.dist.config.js",
    "dist:style": "gulp --gulpfile build/build-style.js",
    "dist:lib": "gulp --gulpfile build/lib.js",
    "mdist:lib": "gulp --gulpfile build/m.lib.js",
    "lib:pc": "cross-env NODE_ENV=production webpack --config build/webpack.lib.config.js",
    "lib:m": "cross-env NODE_ENV=production webpack --config build/webpack.m.lib.config.js",
    "commit": "git-cz",
    "ci:patch": "develop/cicd/global.sh patch",
    "ci:patch:pc": "develop/cicd/global.sh patch H3Yun_PC",
    "ci:patch:mb": "develop/cicd/global.sh patch Mobile",
    "ci:pre": "develop/cicd/global.sh prerelease",
    "ci:pre:pc": "develop/cicd/global.sh prerelease H3Yun_PC",
    "ci:pre:mb": "develop/cicd/global.sh prerelease Mobile",
    "ci:nopb": "develop/cicd/noPublish.sh",
    "ci:nopb:pc": "develop/cicd/noPublish.sh H3Yun_PC",
    "ci:nopb:mb": "develop/cicd/noPublish.sh Mobile",
    "addVersion": "node develop/cicd/addVersion",
    "pushMsg": "node develop/cicd//pushMsg",
    "jenkins": "node develop/cicd/runJenkins",
    "perf": "node develop/tester",
    "lint": "eslint --fix --ext .js,.vue src",
    "tslint": "tslint --type-check --fix --project .",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run lint && npm run tslint && npm run unit",
    "remove:lib": "rimraf lib",
    "auto:docs": "node ./cicd/xlsx2md/do.js",
    "unitTest": "jest"
  },
